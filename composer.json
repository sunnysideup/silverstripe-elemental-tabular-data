{
    "name": "sunnysideup/elemental-tabular-data",
    "description": "Allows adding tabular data to a page using Elemental",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "Silverstripe",
        "Silverstripe CMS",
        "table",
        "tabular",
        "elemental",
        "blocks",
        "data"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\ElementalTabularData\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "bin": [
        "bin/sake-npm-build-elemental-tabular-data.sh",
        "bin/sake-npm-watch-elemental-tabular-data.sh"
    ],
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
